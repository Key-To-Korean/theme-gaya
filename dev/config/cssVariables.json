{
	"variables": {
		"background-color-light": "#f4f7f6",
		"background-color-dark": "#32373c",
		"border-main-color": "#E0E3E3", 
		"placeholder-color": "#428bca",
		"global-font-color": "#555",
		"global-header-color": "#32373c",
		"global-font-family": "'Nanum Gothic', sans-serif",
		"global-font-size": "18",
		"global-font-size-sm": "16",
		"global-font-line-height": "1.8",
		"highlight-font-family": "'Noto Sans', sans-serif",
		"content-width": "45",
		"dropdown-symbol-width": "0.7em",
		"small-font-size": "12",
		"regular-font-size": "15",
		"large-font-size": "24",
		"larger-font-size": "36",
		"image-hover-shadow": "rgba(100,108,116,0.2) 0px 2px 2px, rgba(100,108,116,0.2) 0px 4px 4px, rgba(100,108,116,0.2) 0px 8px 8px, rgba(100,108,116,0.2) 0px 16px 16px, rgba(100,108,116,0.2) 0px 32px 32px, rgba(100,108,116,0.2) 0px 64px 64px",
		"material-teal-a400": "#1de9b6",
		"material-teal-a700": "#00bfa5",
		"k2k-black": "#000000",
		"gaya-white": "#ffffff",
		"gaya-grey0": "#606f7b",
		"gaya-grey1": "#7B8792",
		"gaya-grey2": "#8795A1",
		"gaya-grey3": "#95A4B1",
		"gaya-grey4": "#A9BAC9",
		"gaya-grey5": "gainsboro",
		"gaya-grey-rgb": "135,149,161",
		"gaya-gold": "#CF8031",
		"gaya-teal": "",
		"gaya-teal-dk": "#00897b",
		"gaya-teal-a": "#00bfa5",
		"gaya-amber-dk": "#ff8f00",
		"gaya-amber-a": "#ffab00",
		"gaya-pink-dk": "#e91e63",
		"gaya-pink-a": "#ff4081",
		"gaya-purple-dk": "#9c27b0",
		"gaya-purple-a": "#e040fb",
		"gaya-blue-dk": "#0277bd",
		"gaya-blue-a": "#00b0ff",
		"gaya-box-shadow-lt": "0 0 1px #606f7b",
		"gaya-text-shadow": "rgba(0, 0, 0, 0.2) 0px 1px 2px",
		"gradient-teal": "-webkit-gradient(linear,left top,left bottom,from(#00bfa5),to(#00897b)); background-image: linear-gradient(180deg,#00bfa5,#00897b);",
		"gradient-amber": "-webkit-gradient(linear,left top,left bottom,from(@amber),to(@amber-dark)); background-image: linear-gradient(180deg,@amber,@amber-dark);",
		"gradient-pink": "-webkit-gradient(linear,left top,left bottom,from(@pink),to(@pink-dark)); background-image: linear-gradient(180deg,@pink,@pink-dark);",
		"gradient-purple": "-webkit-gradient(linear,left top,left bottom,from(@purple),to(@purple-dark)); background-image: linear-gradient(180deg,@purple,@purple-dark);",
		"gradient-blue": "-webkit-gradient(linear,left top,left bottom,from(@blue),to(@blue-dark)); background-image: linear-gradient(180deg,@blue,@blue-dark);",
		"gradient-grey": "-webkit-gradient(linear,left top,left bottom,from(@grey-lighter),to(@grey)); background-image: linear-gradient(180deg,@grey-lighter,@grey);"
	},
	"queries": {
		"narrow-menu-query": "screen and (max-width: 37.5em)",
		"wide-menu-query": "screen and (min-width: 37.5em)",
		"content-query": "screen and (min-width: 48em)",
		"sidebar-query": "screen and (min-width: 60em)",
		"widescreen-query": "screen and (min-width: 80em)"
	},
	"gradients": {
		"gradient-teal": "background-image: -webkit-gradient(linear,left top,left bottom,from(@teal),to(@teal-dark)); background-image: linear-gradient(180deg,@teal,@teal-dark);",
		"gradient-amber": "background-image: -webkit-gradient(linear,left top,left bottom,from(@amber),to(@amber-dark)); background-image: linear-gradient(180deg,@amber,@amber-dark);",
		"gradient-pink": "background-image: -webkit-gradient(linear,left top,left bottom,from(@pink),to(@pink-dark)); background-image: linear-gradient(180deg,@pink,@pink-dark);",
		"gradient-purple": "background-image: -webkit-gradient(linear,left top,left bottom,from(@purple),to(@purple-dark)); background-image: linear-gradient(180deg,@purple,@purple-dark);",
		"gradient-blue": "background-image: -webkit-gradient(linear,left top,left bottom,from(@blue),to(@blue-dark)); background-image: linear-gradient(180deg,@blue,@blue-dark);",
		"gradient-grey": "background-image: -webkit-gradient(linear,left top,left bottom,from(@grey-lighter),to(@grey)); background-image: linear-gradient(180deg,@grey-lighter,@grey);"
	}
}
